CREATE MEMORY TABLE PUBLIC.STUDENT(MATRICULATIONNO INTEGER NOT NULL PRIMARY KEY,FIRSTNAME CHARACTER(50),LASTNAME CHARACTER(50),COURSENO INTEGER NOT NULL,AVERAGEGRADE DOUBLE)
CREATE MEMORY TABLE PUBLIC.GRADES(MATRICULATIONNO INTEGER NOT NULL,SUBJECTNO INTEGER NOT NULL,GRADE DOUBLE,FOREIGN KEY(MATRICULATIONNO) REFERENCES PUBLIC.STUDENT(MATRICULATIONNO))
CREATE MEMORY TABLE PUBLIC.COURSE(COURSENO INTEGER NOT NULL PRIMARY KEY,COURSENAME CHARACTER(100))
CREATE MEMORY TABLE PUBLIC.SUBJECT(SUBJECTNO INTEGER NOT NULL PRIMARY KEY,SUBJECTNAME CHARACTER(100),CREDITPOINTS INTEGER NOT NULL,COURSENO INTEGER NOT NULL,PROFESSORNO INTEGER NOT NULL,FOREIGN KEY(COURSENO) REFERENCES PUBLIC.COURSE(COURSENO))
