ALTER TABLE PUBLIC.STUDENT ADD FOREIGN KEY(COURSENO) REFERENCES PUBLIC.COURSE(COURSENO)
ALTER TABLE PUBLIC.GRADES ADD FOREIGN KEY(SUBJECTNO) REFERENCES PUBLIC.SUBJECT(SUBJECTNO)
ALTER TABLE PUBLIC.SUBJECT ADD FOREIGN KEY(PROFESSORNO) REFERENCES PUBLIC.PROFESSOR(PROFESSORNO)
SET SCHEMA PUBLIC
CREATE TRIGGER PUBLIC.T_UPDATEGRADE AFTER UPDATE ON PUBLIC.GRADES REFERENCING NEW ROW AS NEWROW FOR EACH ROW UPDATE PUBLIC.STUDENT SET AVERAGEGRADE=(SELECT AVG(GRADE)FROM PUBLIC.GRADES WHERE GRADES.MATRICULATIONNO=NEWROW.MATRICULATIONNO)WHERE STUDENT.MATRICULATIONNO=NEWROW.MATRICULATIONNO
CREATE TRIGGER PUBLIC.T_DELETEGRADE AFTER DELETE ON PUBLIC.GRADES REFERENCING OLD ROW AS OLDROW FOR EACH ROW UPDATE PUBLIC.STUDENT SET AVERAGEGRADE=(SELECT AVG(GRADE)FROM PUBLIC.GRADES WHERE GRADES.MATRICULATIONNO=OLDROW.MATRICULATIONNO)WHERE STUDENT.MATRICULATIONNO=OLDROW.MATRICULATIONNO
CREATE TRIGGER PUBLIC.T_INSERTGRADE AFTER INSERT ON PUBLIC.GRADES REFERENCING NEW ROW AS NEWROW FOR EACH ROW UPDATE PUBLIC.STUDENT SET AVERAGEGRADE=(SELECT AVG(GRADE)FROM PUBLIC.GRADES WHERE GRADES.MATRICULATIONNO=NEWROW.MATRICULATIONNO)WHERE STUDENT.MATRICULATIONNO=NEWROW.MATRICULATIONNO
